programa
{
	funcao inicio()
	{
		real X, Y, Z
		escreva("Valor de X:")
		leia(X)
		escreva("Valor de Y:")
		leia(Y)

		Z = X*X + Y*Y

		escreva("A soma dos quadrados é:", Z)
	}
}
